pragma ever-solidity ^0.66.0;


interface IUser {

    function onRoomCreated() external;
    function onWithdraw() external;
    function onTokenDeposited(address token, uint128 amount) external;

    function onMessage(uint messageHash, bool deployed) external;
    function onReact(uint32 reaction, bool undo) external;
    function onReputationChanged(uint64 serverID, int8 change, int64 current) external;

    function onPermissionChanged(address entity, bool removed) external;

}
